{
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "explorer.confirmDelete": false,
  "editor.acceptSuggestionOnEnter": "off",
  "explorer.autoReveal": false,
  // Helps speed things up/
  "search.exclude": {
    "**/.git": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "bazel-*": true,
    "sorbet/shims/autogenerated": true,
    "vendor": true
  },
  // production sorbet
  "sorbet.extraArgs": [],
  "sorbet.logLevel": 0,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "bazel-*": true,
    "vendor": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "vendor": true,
    "bazel-*": true,
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "files.useExperimentalFileWatcher": true,
  "workbench.statusBar.visible": true,
  "editor.rulers": [80],
  "editor.minimap.enabled": false,
  "git.autofetch": true,
  "workbench.sideBar.location": "left",
  "todo.embedded.provider": "ag",
  "todo.embedded.include": [],
  "breadcrumbs.enabled": true,
  "files.associations": {
    "*.json": "jsonc"
  },
  "explorer.confirmDragAndDrop": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "flow.enabled": false,
  "metals.scalafmtConfigPath": ".scalafmt",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "crystal-lang.hover": true,
  "crystal-lang.implementations": true,
  "[crystal]": {
    "editor.formatOnSave": false
  },
  "[bazel]": {
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.formatOnSave": true
  },
  "go.formatTool": "goimports",
  "clangd.path": "/usr/local/opt/llvm@8/bin/clangd",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.hovers.enabled": false,
  "vs-kubernetes": {
    "vs-kubernetes.helm-path": "/Users/joey/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.minikube-path": "/Users/joey/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.draft-path": "/Users/joey/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vs-kubernetes.kubectl-path": "/Users/joey/.vs-kubernetes/tools/kubectl/kubectl",
    "vs-kubernetes.kubectl-path.mac": "/Users/joey/.vs-kubernetes/tools/kubectl/kubectl",
    "vs-kubernetes.minikube-path.mac": "/Users/joey/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.helm-path.mac": "/Users/joey/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.draft-path.mac": "/Users/joey/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vscode-kubernetes.helm-path.mac": "/Users/joey/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vscode-kubernetes.kubectl-path.mac": "/Users/joey/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path.mac": "/Users/joey/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vscode-kubernetes.minikube-path-mac": "/Users/joey/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.crd-code-completion": "enabled"
  },
  // This ensures all new tabs are persistent, getting rid of the default behavior
  // where new tabs are in italics and can be replaced by the next new file you open.
  "workbench.editor.enablePreview": false,
  // Otherwise VSCode will close files you created when you change branches.
  "workbench.editor.closeOnFileDelete": false,
  // This is just pretty. To enable this, you need to install the vscode-icons extension first. This can be done in the Extensions panel (cmd+shift+X on OSX).
  "workbench.iconTheme": "vscode-icons",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.renderWhitespace": "boundary",
  "cSpell.userWords": [
    "autokuma",
    "Beelink",
    "Cutover",
    "dataset",
    "deduplicated",
    "Deduplicator",
    "Denorm",
    "denormalized",
    "diffable",
    "dockerproxy",
    "eques",
    "Flink",
    "Gitea",
    "gluetun",
    "herokuapp",
    "homeassistant",
    "iamckn",
    "Immich",
    "infisical",
    "Klipper",
    "Kryo",
    "kuma",
    "kustomization",
    "Lookback",
    "mosquitto",
    "Optane",
    "pereira",
    "pizzly",
    "privs",
    "pymongo",
    "qtree",
    "Radxa",
    "raidz",
    "realtimeapi",
    "Rockchip",
    "Rollup",
    "Rollups",
    "Ryzen",
    "Semigroup",
    "Serde",
    "Serializer",
    "Serializers",
    "servarr",
    "splitwise",
    "stakater",
    "Stateful",
    "struct",
    "tailnet",
    "tailscale",
    "tfvars",
    "traefik",
    "unhexlify",
    "venv",
    "yedit",
    "ynab",
    "youneedabudget"
  ],
  "editor.renderControlCharacters": false,
  "C_Cpp.updateChannel": "Insiders",
  "terminal.integrated.inheritEnv": false,
  "rust-client.rustupPath": "/Users/joey/.cargo/bin/rustup",
  "gitlens.insiders": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [".terraform/**/*", "**/.terraform/**/*"]
  },
  "terraform.languageServer": {
    "external": true,
    "args": ["serve"]
  },
  "java.semanticHighlighting.enabled": true,
  "java.requirements.JDK11Warning": false,
  "go.useLanguageServer": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "yaml.format.enable": false,
  "editor.formatOnSave": true,
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter-notebook",
      "filenamePattern": "*.ipynb"
    }
  ],
  "editor.tabSize": 2,
  "python.languageServer": "Default",
  "update.mode": "none",
  "go.toolsManagement.autoUpdate": true,
  "workbench.colorTheme": "Spinel",
  "mssql.connections": [
    {
      "server": "{{put-server-name-here}}",
      "database": "{{put-database-name-here}}",
      "user": "{{put-username-here}}",
      "password": "{{put-password-here}}"
    }
  ],
  "C_Cpp.intelliSenseEngine": "Disabled",
  "jestrunner.jestCommand": "yarn test",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "bracketPairColorizer.depreciation-notice": false,
  "cSpell.language": "en,en-GB",
  "sonarlint.rules": {
    "python:S5542": {
      "level": "off"
    },
    "kubernetes:S6873": {
      "level": "off"
    },
    "kubernetes:S6892": {
      "level": "off"
    },
    "kubernetes:S6864": {
      "level": "off"
    },
    "kubernetes:S6870": {
      "level": "off"
    },
    "kubernetes:S6897": {
      "level": "off"
    },
    "kubernetes:S6596": {
      "level": "off"
    }
  },
  "ruby.pathToBundler": "bundle",
  "ruby.useBundler": true,
  "ruby.rubocop.useBundler": true,
  "sorbet.enabled": true,
  "sorbet.lspConfigs": [
    {
      "id": "stable",
      "name": "Sorbet",
      "description": "Stable Sorbet Ruby IDE features",
      "cwd": "${workspaceFolder}",
      "command": ["bundle", "exec", "srb", "typecheck", "--lsp"]
    },
    {
      "id": "beta",
      "name": "Sorbet (Beta)",
      "description": "Beta Sorbet Ruby IDE features",
      "cwd": "${workspaceFolder}",
      "command": [
        "bundle",
        "exec",
        "srb",
        "typecheck",
        "--lsp",
        "--enable-all-beta-lsp-features"
      ]
    },
    {
      "id": "experimental",
      "name": "Sorbet (Experimental)",
      "description": "Experimental Sorbet Ruby IDE features (warning: crashy, for developers only)",
      "cwd": "${workspaceFolder}",
      "command": [
        "bundle",
        "exec",
        "srb",
        "typecheck",
        "--lsp",
        "--enable-all-experimental-lsp-features"
      ]
    }
  ],
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "redhat.telemetry.enabled": true,
  "python.pythonPath": "/usr/local/bin/python3",
  "sorbet.highlightUntyped": true,
  // "sorbet.selectedLspConfigId": "stable-local",
  // "sorbet.userLspConfigs": [
  //   {
  //     "id": "stable-local",
  //     "name": "Sorbet",
  //     "description": "Stable Sorbet Ruby IDE features",
  //     "cwd": "${workspaceFolder}",
  //     "command": [
  //       "${workspaceFolder}/bin/bundle",
  //       "exec",
  //       "srb",
  //       "typecheck",
  //       "--lsp"
  //     ]
  //   }
  // ],
  "rubyLsp.rubyVersionManager": {
    "identifier": "none"
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.insertSpaces": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.rulers": [120],
    "editor.semanticHighlighting.enabled": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "makefile.configureOnOpen": false
  // "rubyLsp.rubyVersionManager": "custom",
